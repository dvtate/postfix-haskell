"./prelude.phs" require $prelude =

# String class
( I32 I32 ) class $Str =
((Str): 1 ) (: unpack pop ) $size fun
((Str): 1 ) (: unpack swap pop ) $addr fun

# Overload cast operator to convert string literals
((Syntax:Str Syntax:Type): Str == ) (:
    ( $lit $T ) =
    ( lit ) T make
) $prelude.cast fun

