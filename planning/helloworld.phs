# Import stuff from modules
"stdlib/prelude.phs" require use
"stdlib/io.phs"      require $io =
"stdlib/str.phs"     require use

# Lazy evaluated IO action
(: "Testing" StaticStr cast io.print )
(: 888.5 io.print ) >> $action =

# Export that performs IO
( Unit io.IO ) (:
    # Invoke IO action
    action

    # Print string
    "Hello, world!" StaticStr cast io.print

    # Print an i32
    123456 io.print
) $main export

